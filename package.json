    "build": "tailwindcss -i tailwind.css -o public/styles.css -m && templ generate && go build -o bin/shorter"
  },
  "author": "ozokuz",
  "license": "MIT",
  "devDependencies": {
    "tailwindcss": "^3.4.10"
  }
}
